class Main {
    public static void main(String[] args) {
        int a = 36;
        int b = 60;
        
        int g = a > b ? a : b;
        int s = a > b? b : a;
        
        while(s>0){
            int temp = s;
            s = g%s;
            g = temp;
        }
        System.out.println(g);
        
    }
}
